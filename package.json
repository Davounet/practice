    "tabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "sortAttributes": true,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "css"
